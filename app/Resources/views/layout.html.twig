                        <a ui-sref="login">
                            <i class="fa fa-credit-card"></i>
                            <span class="title">Sales Orders</span>
                        </a>
                    </li>
                    {% endif %}
                    {% if is_granted('ROLE_PURCHASE_ORDERS_LIST') %}
